DECLARE GLOBAL TEMPORARY TABLE MOYA
    ( PILOT   BIGINT NOT NULL GENERATED BY DEFAULT AS IDENTITY
    )
;

DECLARE GLOBAL TEMPORARY TABLE MOYA
    ( PILOT   BIGINT NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1)
    , DARGO   CHARACTER (255) NOT NULL WITH DEFAULT
    , ZHANN   DECIMAL (9,5) NOT NULL WITH DEFAULT 2.71828
    , RYGEL   FLOAT
    )
    CCSID UNICODE
    ON COMMIT PRESERVE ROWS
    NOT LOGGED ON ROLLBACK PRESERVE ROWS
;

DECLARE GLOBAL TEMPORARY TABLE MOYA
    ( PILOT   BIGINT NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1)
    , DARGO   CHARACTER (255) NOT NULL WITH DEFAULT
    , ZHANN   DECIMAL (9,5) NOT NULL WITH DEFAULT 2.71828
    , RYGEL   FLOAT
    )
    USING TYPE DEFAULTS
    CCSID UNICODE
    ON COMMIT PRESERVE ROWS
    NOT LOGGED ON ROLLBACK PRESERVE ROWS
;

DECLARE GLOBAL TEMPORARY TABLE TALYN
    LIKE MOYA
;

DECLARE GLOBAL TEMPORARY TABLE TALYN
    LIKE MOYA EXCLUDING IDENTITY COLUMN ATTRIBUTES
;

DECLARE GLOBAL TEMPORARY TABLE TALYN
    LIKE MOYA EXCLUDING COLUMN DEFAULTS
;

DECLARE GLOBAL TEMPORARY TABLE TALYN
    LIKE MOYA EXCLUDING IDENTITY COLUMN ATTRIBUTES
              INCLUDING DEFAULTS
;

DECLARE GLOBAL TEMPORARY TABLE FARSCAPE1
    AS (SELECT * FROM HOUSTON WHERE PROJECT = 'FARSCAPE')
       WITH NO DATA
;


