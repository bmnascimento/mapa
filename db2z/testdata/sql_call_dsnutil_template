CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE RIVIA DSN(&DB..&TS..D&DATE.)
    UNIT ZZ9PZA  EXPDL '2100001'
','');

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE RIVIA DSN(&DB..&TS..D&DATE.)
    UNIT ZZ9PZA  EXPDL '2100001'','');

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , 'TEMPLATE AB2 DSN '&DB..&SN..Y&YEAR(3)..COPY&LR.&PB..P&PART(3,3).'','');

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , 'TEMPLATE CINTRA DSN 'BANG.&DB..&TS..RESUME' DSNTYPE(PDS) UNIT(SYSDA)','');

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , 'TEMPLATE CINTRA DSN BANG.&DB..&TS..RESUME DSNTYPE(PDS) UNIT(SYSDA)','');

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE
ARETUZA
DSN (''X&DB(1,3)..&DB..&SN(+1)'')
UNIT SYSALLDA
MODELDCB ('DB.MODEL')
BUFNO 30
DATACLAS DB
MGMTCLAS MLONG
STORCLAS SDB
EXPDL '20460101'
VOLUMES (DB001,DB002,DB003)
VOLCNT 3
UNCNT 3
GDGLIMIT 255
DISP(NEW,CATLG,DELETE)
LIMIT(255 GB, CINTRA)
TIME UTC
BLKSZLIM 28K
SPACE(100,100) CYL
PCTPRIME 100
MAXPRIME 100
NBRSECND 100
DIR 100
DSNTYPE EXTPREF
EATTR

    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE
ARETUZA
DSN (''X&DB(1,3)..&DB..&SN(+1'')
UNIT SYSALLDA
MODELDCB ('DB.MODEL')
BUFNO 30
DATACLAS DB
