STORCLAS SDB
EXPDL '20460101'
VOLUMES (DB001,DB002,DB003)
VOLCNT 3
UNCNT 3
GDGLIMIT 255
DISP(NEW,CATLG,DELETE)
LIMIT(255 GB, CINTRA)
TIME UTC
BLKSZLIM 28K
STACK YES
TRTCH NOCOMP
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE
ARETUZA
DSN (''X&DB(1,3)..&DB..&SN(+1'')
UNIT SYSALLDA
MODELDCB ('DB.MODEL')
BUFNO 30
DATACLAS DB
MGMTCLAS MLONG
STORCLAS SDB
EXPDL '20460101'
VOLUMES (DB001,DB002,DB003)
VOLCNT 3
UNCNT 3
GDGLIMIT 255
DISP(NEW,CATLG,DELETE)
LIMIT(255 GB, CINTRA)
TIME UTC
BLKSZLIM 28K
STACK YES
TRTCH NOCOMP
SUBSYS RIVIA
  LRECL 27998
  RECFM VB
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE
ARETUZA
PATH /dev1/db2/path
  FILEDATA RECORD
  RECFM VB
  LRECL 27998
  PATHOPTS(OCREAT,OWRONLY)
  PATHMODE(
            SIRUSR
          , SIWUSR
          , SIXUSR
          , SIRWXU
          , SIRGRP
          , SIXGRP
          , SIRWXG
          , SIROTH
          , SIWOTH
          , SIXOTH
          )
   PATHDISP(KEEP,DELETE)
    '
  , ''
  );

