CALL SYSPROC.DSNUTILS(
    'DBB.STUFF'
  , 'NO'
  , 'BACKUP SYSTEM'
  , '');

CALL SYSPROC.DSNUTILS(
    'DBB.STUFF'
  , 'NO'
  , 'BACKUP SYSTEM FULL'
 , '');

CALL SYSPROC.DSNUTILS(
    'DBB.STUFF'
  , 'NO'
  , 'BACKUP SYSTEM DATA ONLY'
  , '');

CALL SYSPROC.DSNUTILS(
    'DBB.STUFF'
  , 'NO'
  , 'BACKUP SYSTEM FULL ALTERNATE_CP(POOL1)'
  , '');

CALL SYSPROC.DSNUTILS(
    'DBB.STUFF'
  , 'NO'
  , '
BACKUP SYSTEM DATA ONLY ALTERNATE_CP(POOL1) DBBSG(STG1) LGBSG(STG2)
  ESTABLISH FCINCREMENTAL FORCE
    '
  , '');
CALL SYSPROC.DSNUTILS(
    'DBB.STUFF'
  , 'NO'
  , 'BACKUP SYSTEM DATA ONLY END FCINCREMENTAL'
  , '');

CALL SYSPROC.DSNUTILS(
    'DBB.STUFF'
  , 'NO'
  , 'BACKUP SYSTEM FULL DUMP DUMPCLASS(DMPCLS1)'
  , '');

CALL SYSPROC.DSNUTILS(
    'DBB.STUFF'
  , 'NO'
  , 'BACKUP SYSTEM FULL DUMP DUMPCLASS(DMPCLS1,DMPCLS2,DMPCLS3)'
  , '');

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , 'BACKUP SYSTEM FULL DUMP DUMPCLASS(DMPCLS1,DMPCLS2,DMPCLS3) FORCE'
  , '');

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , 'BACKUP SYSTEM DATA ONLY DUMPONLY TOKEN(X'BADF00D0')'
  , '');

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
BACKUP SYSTEM DATA ONLY DUMPONLY TOKEN(X'BADF00D0')
  DUMPCLASS(DMPCLS1,DMPCLS2,DMPCLS3)
    '
  , '');
  
