CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REBUILD INDEX (
  GNOMON.NEITH
  , "GNOMON".TUBMAN PART 1
  , GNOMON."HUNTER")
  SHRLEVEL CHANGE MAXRO DEFER LONGLOG TERM DELAY 42
  DRAIN_WAIT 7 RETRY 11 RETRY_DELAY 13
  CLONE
  SCOPE ALL
  REUSE
  SORTDEVT WRK
  SORTNUM 17
  STATISTICS REPORT YES FREQVAL NUMCOLS 3 FREQVAL NUMCOLS 5 UPDATE ACCESSPATH
  FLASHCOPY CONSISTENT
  RBALRSN_CONVERSION NONE'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'REBUILD INDEX (ALL) TABLESPACE GNOMON.'HUNTER''
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REBUILD INDEX (GNOMON.NEITH PART 1)
  STATISTICS FORCEROLLUP YES
  SCOPE ALL
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'REBUILD INDEXSPACE (GNOMON.TUBMAN) CLONE'
  , ''
  );




