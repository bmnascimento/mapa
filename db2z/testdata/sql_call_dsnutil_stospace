CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'STOSPACE STOGROUP 'INARA SERRA''
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'STOSPACE STOGROUP('INARA SERRA')'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'STOSPACE STOGROUP('INARA SERRA', 'MALCOLM REYNOLDS')'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'STOSPACE STOGROUP JAYNECOBB'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'STOSPACE STOGROUP (MAL, JAYNE, WASH, ZOE, KAYLEE)'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'STOSPACE STOGROUP *'
  , ''
  );


