CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REORG TABLESPACE DB1.TS1 SORTDATA 
    DISCARD
    FROM TABLE "GNOMON"."NEITH"
        WHEN (
               COL1 LIKE '%;%' 
         AND   COL2 IN ('A', 'B' , 'C' )
          OR   (COL3 BETWEEN 'X' AND 'Z' AND
                COL4 NOT BETWEEN 'L' AND 'N')
             )
    FROM   TABLE "GNOMON"."TUBMAN"
        WHEN (COL1 LIKE '%;%' AND COL2 IN ('A', 'B' , 'C' ))
    FROM 
    TABLE "GNOMON"."HUNTER"
        WHEN (COL1 LIKE '%;%' AND COL2 IN ('A', 'B' , 'C' ))
    SHRLEVEL CHANGE
    DEADLINE CURRENT_TIMESTAMP WITH TIME ZONE - 3 DAYS + 4 HOURS - 5 MINUTES + 6 SECONDS
      DRAIN_WAIT 7 
      RETRY 8 
      RETRY_DELAY 9 
      TIMEOUT ABEND 
      LOGRANGES YES 
      DRAIN_ALLPARTS YES
      SWITCHTIME CURRENT_DATE + 4 MONTHS
        NEWMAXRO NONE
      MAXRO DEFER
      DRAIN WRITERS
      LONGLOG DRAIN
      DELAY 13
      LASTLOG YES
      MAPPINGTABLE 'MAPPINGTABLE'
      MAPPINGDATABASE "MAPPINGDATABASE"
    UNLOAD CONTINUE
      KEEPDICTIONARY
      STATISTICS
          TABLE(GNOMON.NEITH)
          TABLE('GNOMON'.TUBMAN) TABLESAMPLE SYSTEM 
              COLUMN ("C1", "C2", "C3") 
              COLGROUP ('C4', 'C5', 'C6') 
              COLGROUP ("C7", "C8")
          TABLE(GNOMON.'HUNTER') TABLESAMPLE SYSTEM AUTO
          TABLE("GNOMON".LONNROT)
          TABLE(GNOMON."BURTON")
          TABLE("GNOMON"."DIANA")
          TABLE(KYRIAKOS)
          INDEX( GNOMON.NEITH
               , 'GNOMON'.TUBMAN KEYCARD
               , GNOMON.'HUNTER'
               , 'GNOMON'.'LONNROT' FREQVAL NUMCOLS 1
               , "GNOMON".TUBMAN
               , GNOMON."HUNTER" FREQVAL NUMCOLS 1 COUNT 7 MOST
               , "GNOMON"."LONNROT")
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'REORG TABLESPACE 'DB1'.TS2'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'REORG TABLESPACE DB1.'TS3''
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'REORG TABLESPACE 'DB1'.'TS4''
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , "REORG TABLESPACE "DB1".TS2"
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , "REORG TABLESPACE DB1."TS3""
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , "REORG TABLESPACE "DB1"."TS4""
  , ''
  );


