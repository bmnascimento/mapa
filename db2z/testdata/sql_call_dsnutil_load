CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , 'LOAD DATA
     RESUME YES
     INTO TABLE KAER.MORHEN
     (GERALT    POSITION (1:3)     CHAR(3),
      YENNEFER POSITION (4:39)    CHAR(36),
      CIRI     POSITION (40:45)   CHAR(6),
      FRINGILA POSITION (46:48)   CHAR(3),
      VESEMIR POSITION (49:64)   CHAR(16))
  '
  , '');

