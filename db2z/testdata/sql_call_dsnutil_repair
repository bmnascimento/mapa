CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
LOCATE 
  TABLESPACE "DB".TS1 PART 1 PAGE X'0001'
    VERIFY OFFSET X'1' DATA X'0101'
  TABLESPACE DB."TS2" KEY '2222' INDEX "I2"
    VERIFY OFFSET 2 DATA '2222'
  INDEXSPACE 'I3' PART 3 PAGE 33
    REPLACE OFFSET X'3' DATA '3333'
  INDEX 'I4' PART 4 PAGE X'44'
    DELETE DATAONLY
    DUMP OFFSET X'4444' LENGTH X'44' PAGES X'4'
    DUMP PAGES *
    '
  , ''
  );


  
